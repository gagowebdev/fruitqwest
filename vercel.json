{
  "version": 2,
  "builds": [
      {
          "src": "dist/main.js",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "dist/main.js"
      }
  ],
  "functions": {
      "api/index.js": {
          "runtime": "nodejs18.x"
      }
  }
}
